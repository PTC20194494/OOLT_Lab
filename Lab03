
public class Aims {

	public static void main(String[] args) {
		Order anorder = new Order();
		DigitalVideoDisk dvd1 = new DigitalVideoDisk("The Lion King");
		dvd1.setCategory("Animation");
		dvd1.setCost(60.63f);
		dvd1.setDirector("James Cameron");
		dvd1.setLength(90);
		
		anorder.addDigitalVideoDisk(dvd1);
		
		DigitalVideoDisk dvd2 = new DigitalVideoDisk("Star Wars");
		dvd2.setCategory("Science Fiction");
		dvd2.setCost(16.8f);
		dvd2.setDirector("Geogre Lucas");
		dvd2.setLength(75);
		
		anorder.addDigitalVideoDisk(dvd2);
		
		DigitalVideoDisk dvd3 = new DigitalVideoDisk("Aladin");
		dvd3.setCategory("Animation");
		dvd3.setCost(18.8f);
		dvd3.setDirector("John Watts");
		dvd3.setLength(85);
		
		anorder.addDigitalVideoDisk(dvd3);
		
		anorder.OutPut();

	}

}
